  // Analytics Tracking - START
  function tracking(location) {
    window.ga("send", "event", "PreFooter", "click", location);
  }

  $(".footer_nav_link").click(function() {
    window.ga("send", "event", "Footer", "click", $(this).html());
  });
  // Analytics Tracking - END

  // Ada Chatbot ZEN-883
  if (
    !/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) &&
    window.location.href.indexOf("en-us") !== -1
  ) {
    if (window.location.href.indexOf("requests") === -1) {
      var script = document.createElement("script");

      script.src = "https://static.ada.support/embed.js";
      script.setAttribute("id", "__ada");
      script.setAttribute("data-handle", "squarespace");

      var ref = document.querySelector("script");

      ref.parentNode.insertBefore(script, ref);
    }
  }
</script>

